{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base",
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"semi": "off",
		"eol-last": "off",
		"max-len": [
			"error",
			150
		],
		"no-tabs": "off",
		"no-alert": "off",
		"prefer-destructuring": "off"
	}
}